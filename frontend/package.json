    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
