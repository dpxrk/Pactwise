{
  "name": "pactwise-fork",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TURBOPACK=1 next dev --turbo",
    "dev:clean": "rm -rf .next && TURBOPACK=1 next dev --turbo",
    "dev:turbo": "TURBOPACK=1 next dev --turbo",
    "dev:webpack": "next dev",
    "dev:restart": "rm -rf .next node_modules/.cache && TURBOPACK=1 next dev --turbo",
    "dev:fresh": "rm -rf .next node_modules/.cache && TURBOPACK=1 next dev --turbo",
    "dev:standard": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "check-unused": "npx eslint . --ext .ts,.tsx --rule '@typescript-eslint/no-unused-vars: error'",
    "pre-commit": "npm run lint && npm run typecheck",
    "test": "npm run test:frontend",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test:frontend:coverage",
    "test:backend": "echo \"Backend tests pending Supabase integration\"",
    "test:backend:coverage": "echo \"Backend coverage pending Supabase integration\"",
    "test:frontend": "jest --config jest.config.frontend.js",
    "test:frontend:coverage": "jest --config jest.config.frontend.js --coverage",
    "test:frontend:watch": "jest --config jest.config.frontend.js --watch",
    "test:ci": "npm run test:frontend:coverage",
    "test:optimizations": "tsx scripts/test-optimizations.ts",
    "metrics:baseline": "tsx scripts/collect-baseline-metrics.ts",
    "data:generate": "tsx scripts/generate-test-data.ts",
    "data:generate:small": "tsx scripts/generate-test-data.ts small",
    "data:generate:medium": "tsx scripts/generate-test-data.ts medium",
    "data:generate:large": "tsx scripts/generate-test-data.ts large",
    "backup:monitor": "tsx scripts/backup-monitor.ts",
    "backup:test-restore": "tsx scripts/test-restore.ts",
    "health:check": "curl -f http://localhost:3000/api/v1/health || exit 1",
    "bundle:analyze": "tsx scripts/analyze-bundle.ts",
    "bundle:analyze:build": "ANALYZE=true npm run build",
    "bundle:monitor": "tsx scripts/bundle-size-monitor.ts",
    "bundle:report": "npm run build && npm run bundle:monitor"
  },
